{
    "display": "form",
    "components": [
      {
        "label": "Full Name",
        "key": "fullName",
        "type": "textfield",
        "input": true
      },
      {
        "label": "Email",
        "key": "email",
        "type": "email",
        "input": true
      },
      {
        "label": "Phone Number",
        "key": "phoneNumber",
        "type": "phoneNumber",
        "input": true
      },
      {
        "label": "Date of Birth",
        "key": "dob",
        "type": "datetime",
        "input": true,
        "enableTime": false,
        "format": "yyyy-MM-dd"
      },
      {
        "label": "Address",
        "key": "address",
        "type": "textarea",
        "input": true
      },
      {
        "label": "Status",
        "key": "status",
        "type": "select",
        "input": true,
        "dataSrc": "values",
        "data": {
          "values": [
            { "label": "Active", "value": "active" },
            { "label": "Inactive", "value": "inactive" },
            { "label": "Pending", "value": "pending" }
          ]
        }
      },
      {
        "type": "button",
        "label": "Submit",
        "key": "submit",
        "disableOnInvalid": true,
        "input": true,
        "tableView": false
      }
    ]
  }
  